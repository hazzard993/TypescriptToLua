{
    "compilerOptions": {
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "rootDir": "./src",
        "outDir": "./dist",
        "declaration": true,
        "sourceMap": true,
        "target": "es2017",
        "lib": ["es2017"],
        "module": "commonjs",
        "stripInternal": true
    },
    "include": ["src"],
    "exclude": ["src/lualib"]
}
